<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.rinsoz.climbtracker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.rinsoz.climbtracker.RouteListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.rinsoz.climbtracker.RouteDetailActivity"
            android:label="@string/title_route_detail"
            android:parentActivityName="org.rinsoz.climbtracker.RouteListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RouteListActivity" />
        </activity>
    </application>

</manifest>
